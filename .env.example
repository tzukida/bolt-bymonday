# Environment Variables Example
# Copy this file to .env and update the values

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_USE_BACKEND=true

# App Configuration
EXPO_PUBLIC_APP_NAME=ByMonday
EXPO_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
EXPO_PUBLIC_ENABLE_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_ACTIVITY_LOGS=true
EXPO_PUBLIC_ENABLE_USER_MANAGEMENT=true
EXPO_PUBLIC_ENABLE_REPORTS=true

# Development
EXPO_PUBLIC_DEBUG_LOGS=true
EXPO_PUBLIC_MOCK_API_DELAY=1000